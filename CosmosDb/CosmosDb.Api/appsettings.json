{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "PrivateKey": "<secret>",
    "ExpirationInHours": 2
  },
  "CosmosDbSettings": {
    "AccountEndpoint": "<COSMOS_ACCOUNT_ENDPOINT>",
    "ConnectionString": "<COSMOS_CONNECTION_STRING>",
    "Database": "<COSMOS_DATABASE_NAME>",
    "Account": "<COSMOS_ACCOUNT_NAME>"
  },
  "SecuritySettings": {
    "PepperKey": "<secret>"
  }
}
